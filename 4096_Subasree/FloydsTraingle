package Test;
public class Floyds_Traingle 
{
    public void printTriangle(int rows) 
    {
        int num = 1;
        System.out.printf("Floyd's triangle of %d rows is : %n", rows);
        for (int i = 1; i <= rows; i++) 
        {
            for (int j = 1; j <= i; j++) 
            {
                System.out.print(num+" ");
                num++;
            }
            System.out.println();
        }
    }
    public static void main(String args[]) 
	{
    	Floyds_Traingle obj=new Floyds_Traingle();
    	obj.printTriangle(5);
    }
}
