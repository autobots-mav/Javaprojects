import java.util.Scanner;

public class FirstWeek {
	private static void star(String c) {
	for(int i=0;i<=6;i++) {
		System.out.println("");
		for(int j=0;j<=i;j++)
		{
		System.out.print(c+" ");
		}
	}
}
	public static void main(String[] args) {
		System.out.println("Enter any symbol :");
		Scanner sc = new Scanner(System.in);
		String c = sc.next();
		star(c);
		sc.close();
	}
}
