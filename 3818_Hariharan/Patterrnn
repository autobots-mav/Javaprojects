import java.util.Scanner;

public class Patternn {
	public static void FloydTriangle(int rows)
	{
		int number = 1;


		for (int i = 1; i <= rows; i++)
		{
			for (int j = 1; j <= i; j++)
			{
				System.out.print(number + " ");
				number++;
			}
			System.out.println();
		}
	}
	public static void main(String args[])
	{
		Patternn.FloydTriangle(5);
	}
}
