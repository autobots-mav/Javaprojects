package assesment;
import java.util.Scanner;

public class pattern {
	public static void pyramid(char n) {
		int i,j;
		for(i=0;i<5;i++)
		{
			for(j=0;j<=i;j++) {
				
				System.out.print(n);
				
			}
			System.out.println();
		}
		
	}

	public static void main(String[] args) {
	
		Scanner sc = new Scanner(System.in);
		char x  = sc.next().charAt(0);
		pattern ob = new pattern();
		ob.pyramid(x);

	}

}
 output:
 #
#
##
###
####
#####
