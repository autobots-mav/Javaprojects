
public class Tak3 {
public static void printFloydTriangle(int rows)
	    {
	        int number = 1;
	        for (int i = 1; i <= rows; i++)
	        {
	            for (int j = 1; j <= i; j++)
	            {
	                System.out.print(number + " ");
	                number++;
	            }
	            System.out.println();
	        }
	    }
public static void main(String args[])
	{
	    	Tak3.printFloydTriangle(5);
    }

}
